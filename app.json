{
  "expo": {
    "name": "Daykeeper",
    "slug": "Daykeeper",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Daykeeper-Icon-Blue.png",
    "scheme": "daykeeperapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.daykeeper-app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Daykeeper-SplashScreen.png",
        "backgroundColor": "#6DB5FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.daykeeperapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Daykeeper-Icon-Blue.png"
    },
    "splash": {
      "image": "./assets/images/Daykeeper-SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#6DB5FF"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
